{
  "version": 2,
  "routes": [
    { "src": "/api/shorten", "dest": "/api/shorten.js" },
    { "src": "/api/resolve", "dest": "/api/resolve.js" },
    { "src": "/file/(.*)", "dest": "/public/viewer.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
